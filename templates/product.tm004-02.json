        "item_3969iU": {
          "type": "item",
          "settings": {
            "title": "Is it foldable for easy storage?",
            "content": "<p>Yes, It's foldable and easy to store.</p>"
          }
        },
        "item_TCzE4d": {
          "type": "item",
          "settings": {
            "title": "How quiet is it?",
            "content": "<p>Less than 40dB when working,just like whispering,super quiet.Don‚Äôt have to worry about a loud motor  in the background of your meetings or when your roomie demands peace and quiet.</p>"
